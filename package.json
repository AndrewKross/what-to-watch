    "moment": "2.27.0",
    "moment-locales-webpack-plugin": "1.2.0",
    "nanoid": "3.1.12",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-loader-spinner": "3.1.14",
