{
  "name": "what-to-watch",
  "version": "1.0.0",
  "homepage": "https://andrewkross.github.io/what-to-watch",
  "private": true,
  "description": "Личный проект «Что посмотреть» от HTML Academy",
  "scripts": {
    "eslint": "npm run eslint.js && npm run eslint.ts",
    "eslint.js": "eslint --ext .jsx --ext .js src/",
    "eslint.ts": "eslint --config .eslintrc-ts.yml --ext .tsx src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest",
    "jest.update": "npm run test.jest -- -u",
    "build": "webpack --mode production",
    "start": "webpack-dev-server",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
